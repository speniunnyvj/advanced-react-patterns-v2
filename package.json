{
  "name": "advanced-react-patterns-v2",
  "version": "1.0.0",
  "description": "The updated version of my advanced react component patterns course",
  "keywords": [],
  "homepage": "https://github.com/kentcdodds/advanced-react-patterns-v2",
  "main": "src/index.js",
  "dependencies": {
    "hoist-non-react-statics": "2.5.0",
    "react": "16.3.0",
    "react-dom": "16.3.0",
    "react-testing-library": "1.9.5",
    "redux": "3.7.2"
  },
  "devDependencies": {
    "react-scripts": "2.0.0-next.b2fd8db8"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app",
    "rules": {
      "no-unused-vars": [
        "warn",
        {
          "argsIgnorePattern": "^_.+|^ignored.+",
          "varsIgnorePattern": "^_.+|^ignored.+",
          "args": "after-used"
        }
      ]
    }
  },
  "browserslist": {
    "development": [
      "last 2 chrome versions",
      "last 2 firefox versions",
      "last 2 edge versions"
    ],
    "production": [
      ">1%",
      "last 4 versions",
      "Firefox ESR",
      "not ie < 11"
    ]
  }
}
